    "factory-girl": "5.0.4",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "jest-config": "24.8.0",
    "lint-staged": "9.2.0",
    "mongodb-memory-server": "5.1.5",
    "nock": "10.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
