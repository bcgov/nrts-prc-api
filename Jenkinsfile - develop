pipeline {
  agent any
  options {
    skipDefaultCheckout()
  }
  stages {
    stage('Building: api (develop branch)') {
      steps {
        script {
          try {
            echo "Building: env.JOB_NAME=${env.JOB_NAME} env.BUILD_ID=${env.BUILD_ID}"
            openshiftBuild bldCfg: 'nrts-prc-api', showBuildLogs: 'true'
          } catch (e) {
            error("Building: Failed: ${e}")
          }
          echo "Building: Success"
        }
      }
    }
  }
}
